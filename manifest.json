"19": "icons/19.png",
"38": "icons/38.png"
},
"default_title": "Twitter One Click Block",
"default_popup": "popup.html"
},
"web_accessible_resources": [ "hakai.mp4" ],
"icons": {
"128": "icons/128.png",
"48": "icons/48.png",
"16": "icons/16.png"
},
"manifest_version": 2,
"update_url": "https://clients2.google.com/service/update2/crx",
"key": ""
}
